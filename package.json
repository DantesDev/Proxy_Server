{
  "name": "my-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write 'src/**/*.{js,ts,json,md}'",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.9"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.8",
    "@types/mongodb": "^4.0.6",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "husky": "^9.1.7",
    "nodemon": "^2.0.4",
    "prettier": "^3.4.2",
    "supertest": "^6.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.3"
  }
}
